        Box.FocusLost:Connect(function() Callback(Box.Text) end)
    elseif Type == "Button" then
        local Btn = Instance.new("TextButton", Section)
        Btn.Size = UDim2.new(0, 80, 0, 18)
        Btn.Position = UDim2.new(1, -90, 0.5, -9)
